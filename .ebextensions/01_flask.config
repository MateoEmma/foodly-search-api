option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: api:app
  aws:elasticbeanstalk:application:environment:
    FLASK_DEBUG: "False"
    PYTHONPATH: "/var/app"
    
container_commands:
  01_download_nltk_data:
    command: "source /var/app/venv/*/bin/activate && python -m nltk.downloader punkt stopwords"